<Window x:Class="SQLMenuBuilder.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SQLMenuBuilder"
        mc:Ignorable="d"
		Title="Главное окно" Height="550" Width="910" Closed="Window_Closed">

	<Window.DataContext>
		<local:MainWindowModelView/>
	</Window.DataContext>

	<Grid x:Name="MainGrid">
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto"/>
		</Grid.RowDefinitions>
		<Menu x:Name="MainMenu" Grid.Row="0">

		</Menu>
		<Frame x:Name="MainFrame" Grid.Row="1" Background="White" Margin="0,19,0,-434">

		</Frame>
	</Grid>
</Window>
